{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "sourceMap": false,
    "allowJs": true,
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "baseUrl": ".",
    "paths": {
      "axios": [
        "node_modules/axios/dist/axios",
        "node_modules/axios/index.d.ts"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
