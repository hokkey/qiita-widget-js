{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "sourceMap": false,
    "allowJs": false,
    "declaration": false,
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "baseUrl": ".",
    "paths": {
      "axios": [
        "node_modules/axios/dist/axios",
        "node_modules/axios/index.d.ts"
      ],
      "localforage": [
        "node_modules/localforage/dist/localforage",
        "node_modules/localforage/typings/localforage.d.ts"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
